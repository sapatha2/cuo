method {
  vmc
  nblock 500
  average { tbdm_basis
    mode obdm
    orbitals {
      magnify 1
      nmo 14
      orbfile iao.orb
      include iao.basis
      centers { useglobal }
    }
    states { 2 6 7 8 9 10 12 13 14 }
  }
}

include gs2.sys
trialfunc {
  slater-jastrow
  wf1 { include ex2_s_Ndet10_gsw0.7_5.slater }
  wf2 { include gs.optjast3 }
}
