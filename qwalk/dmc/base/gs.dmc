method { 
  dmc
  timestep 0.01
  nblock 100 
  tmoves 
  average { tbdm_basis
    orbitals { 
      magnify 1
      nmo 14
      orbfile iao.orb
      include iao.basis
      centers { useglobal } 
    }
    states { 2 6 7 8 9 10 12 13 14 } 
  }
}

include gs.sys
trialfunc { 
  slater-jastrow
  wf1 { include gs.slater } 
  wf2 { include gs.optjast3 } 
}
